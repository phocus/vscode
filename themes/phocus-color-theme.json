{
	"name": "phocus",
	"type": "dark",
	"colors": {
		"foreground": "#FFFFFFDC",
		"focusBorder": "#6940E3",

		"activityBar.background": "#1E1E20",
		"activityBar.activeBorder": "#6940E3",
		"activityBar.dropBorder": "#ffffff3d",
		"activityBar.inactiveForeground": "#ffffff3d",
		"statusBar.background": "#0B0A0B",

		"statusBarItem.remoteBackground": "#6940E3",
		"statusBarItem.hoverBackground": "#ffffff0f",
		"statusBarItem.warningBackground": "#EF9C57",
		"statusBarItem.errorBackground": "#DA5858",

		"sideBar.background": "#0B0A0B",
		"sideBarTitle.foreground": "#ffffff3d",
		"sideBar.border": "#1E1E20",

		"list.activeSelectionBackground": "#6940E3",
		"list.inactiveSelectionBackground": "#ffffff0f",
		"list.dropBackground": "#ffffff0f",
		"list.focusBackground": "#ffffff0f",
		"list.focusOutline": "#ffffff0f",
		"list.hoverBackground": "#ffffff0f",
		"tree.indentGuidesStroke": "#ffffff3d",

		"panel.background": "#0B0A0B",
		"panel.border": "#1E1E20",
		"panel.dropBorder": "#ffffff3d",
		"panelTitle.activeBorder": "#6940E3",
		"panelTitle.inactiveForeground": "#ffffff3d",

		"terminal.ansiRed": "#DA5858",
		"terminal.ansiBrightRed": "#F07575",
		"terminal.ansiYellow": "#EBC747",
		"terminal.ansiBrightYellow": "#F6E67E",
		"terminal.ansiGreen": "#43D665",
		"terminal.ansiBrightGreen": "#67E985",
		"terminal.ansiCyan": "#58DEED",
		"terminal.ansiBrightCyan": "#7EEFFC",
		"terminal.ansiBlue": "#5C8AE6",
		"terminal.ansiBrightBlue": "#7BA5F9",
		"terminal.ansiMagenta": "#6940E3",
		"terminal.ansiBrightMagenta": "#7E54FC",

		"editor.background": "#141415",
		"editorGroupHeader.tabsBackground": "#0B0A0B",
		"tab.activeBackground": "#141415",
		"tab.inactiveBackground": "#0B0A0B",
		"tab.border": "#141415",
		"tab.hoverBackground": "#ffffff0f",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFFFFF3D"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#58DEED"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#FFFFFFDC"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#DA5858"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#FFFFFFDC"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#FFFFFFDC"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#DA5858"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#5C8AE6"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#DA5858"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#DA5858"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#EF9C57"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"punctuation.definition.string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#43D665"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#EBC747"
			}
		},
		{
			"name": "Class Type",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"foreground": "#F6E67E"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#58DEED"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#58DEED"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#DA5858"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#DA5858"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5C8AE6"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#5C8AE6"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
