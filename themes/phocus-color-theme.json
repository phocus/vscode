			],
			"settings": {
				"foreground": "#DA5858"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#6940E3"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
